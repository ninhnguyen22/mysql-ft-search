{
    "name": "nin/mysql-ft-search",
    "type": "library",
    "require": {
        "laravel/scout": "10.x-dev",
        "illuminate/database": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nin\\MySqlFtSearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nin\\MysqlFtSearch\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Nin",
            "email": "ninhnghia2@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Nin\\MySqlFtSearch\\ServiceProvider"
            ],
            "aliases": {
                "FtSchema": "Nin\\MySqlFtSearch\\Facade"
            }
        }
    }
}
